{
  "name": "alnwoks-website",
  "version": "1.0.0",
  "description": "ALNWOKS Solutions Limited - Corporate Website",
  "private": true,
  "keywords": [
    "alnwoks",
    "technology",
    "solutions",
    "enterprise",
    "government",
    "website"
  ],
  "author": "ALNWOKS Solutions Limited",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "npm run build:js && npm run build:css",
    "build:css": "postcss src/css/main.css -o dist/css/main.css",
    "build:js": "webpack --config webpack.config.js --mode=production",
    "watch": "npm run watch:css & npm run watch:js",
    "watch:css": "postcss src/css/main.css -o dist/css/main.css --watch",
    "watch:js": "webpack --config webpack.config.js --mode=development --watch",
    "test": "jest",
    "lint": "eslint \"src/js/**/*.js\"",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "audit": "npm audit --audit-level=high"
  },
  "dependencies": {
    "alpinejs": "^3.13.3",
    "aos": "^2.3.4",
    "express": "^4.18.2",
    "swiper": "^11.0.4",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.3",
    "css-loader": "^6.8.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2"
  }
}
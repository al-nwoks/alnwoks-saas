{
  "name": "alnwoks-website",
  "version": "1.0.0",
  "description": "ALNWOKS Solutions Limited - Corporate Website",
  "main": "index.js",
  "scripts": {
    "dev": "live-server --port=3000 --host=0.0.0.0 --open=index.html",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss src/css/main.css -o dist/css/main.css",
    "build:js": "webpack --mode=production",
    "watch": "npm run watch:css & npm run watch:js",
    "watch:css": "postcss src/css/main.css -o dist/css/main.css --watch",
    "watch:js": "webpack --mode=development --watch",
    "test": "jest",
    "lint": "eslint src/js/**/*.js",
    "format": "prettier --write src/**/*.{js,css,html}"
  },
  "keywords": [
    "alnwoks",
    "technology",
    "solutions",
    "enterprise",
    "government",
    "website"
  ],
  "author": "ALNWOKS Solutions Limited",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "autoprefixer": "^10.4.16",
    "tailwindcss": "^3.3.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "live-server": "^1.2.2"
  },
  "dependencies": {
    "alpinejs": "^3.13.3",
    "aos": "^2.3.4",
    "swiper": "^11.0.4",
    "chart.js": "^4.4.0",
    "gsap": "^3.12.2",
    "express": "^4.18.2"
  },
  "scripts": {
    "dev": "node server.js",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss src/css/main.css -o dist/css/main.css",
    "build:js": "webpack --mode=production",
    "watch": "npm run watch:css & npm run watch:js",
    "watch:css": "postcss src/css/main.css -o dist/css/main.css --watch",
    "watch:js": "webpack --mode=development --watch",
    "test": "jest",
    "lint": "eslint src/js/**/*.js",
    "format": "prettier --write src/**/*.{js,css,html}"
  }
}